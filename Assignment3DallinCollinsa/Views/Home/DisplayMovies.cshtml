
@{
}

@model IEnumerable<Assignment3DallinCollinsa.Models.EnterMoviesModel>
<div>
    <table>
        <thead>
            <tr>
                <th>Category</th>
                <th>Title</th>
                <th>Year</th>
                <th>Director</th>
                <th>Rating</th>
                <th>Edited</th>
                <th>Lent To</th>
                <th>Notes</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Assignment3DallinCollinsa.Models.EnterMoviesModel x in Model)
            {
            <tr>
                <td>@x.category</td>
                <td>@x.title</td>
                <td>@x.year</td>
                <td>@x.director</td>
                <td>@x.rating</td>
                <td>@x.edited</td>
                <td>@x.lentTo</td>
                <td>@x.notes</td>
            </tr>
            }
        </tbody>
    </table>
</div>